	public void operador() {
		// 8 9 7 - + = 10
		
		switch (operador) {
		case'+':
			somar();
			break;
		case '-':
			subtrair();
			break;
		case '/':
			dividir();
			break;
		case '*':
			multiplicar();
			break;
		default: adicionarNumeroNaPilha();
		}
	}

